{
  "name": "mps-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "gen:openapi": "openapi-typescript ../mps-api/build/openapi/openapi.yaml -o ./src/api/generated/schema.d.ts"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0",
    "vue": "3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@iconify-json/lucide": "^1.2.62",
    "@types/node": "24.2.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.4.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "unocss": "66.4.2",
    "vite": "7.1.2",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.0.5"
  }
}
